{
    "name": "RatePage",
    "author": [
        "Ostrzyciel",
        "Polskacafe"
    ],
    "license-name": "MIT",
    "version": "0.1.0",
    "namemsg": "ratePage",
    "descriptionmsg": "ratePage-desc",
    "manifest_version": 1,
    "MessagesDirs": {
        "RatePage": [ "i18n" ]
    },
    "AutoloadClasses": {
        "RatePage": "includes/RatePage.php",
        "RatePageHooks": "includes/RatePageHooks.php",
        "ApiPageRating": "includes/ApiPageRating.php",
        "ApiVoteOnPage": "includes/ApiVoteOnPage.php"
    },
    "ResourceFileModulePaths": {
        "localBasePath": "modules",
        "remoteExtPath": "RatePage/modules"
    },
    "ResourceModules": {
        "ext.ratePage": {
            "scripts": [
                "ext.ratePage.js"
            ],
            "styles": [
                "ext.ratePage.css"
            ],
            "messages": [
                "ratePage",
                "ratePage-desc",
                "ratePage-view-count-label",
                "apihelp-pagerating-summary",
                "apihelp-pagerating-param-pageid",
                "apihelp-pagerating-param-pagetitle",
                "apihelp-voteonpage-summary",
                "apihelp-voteonpage-param-pageid",
                "apihelp-voteonpage-param-pagetitle",
                "apihelp-voteonpage-param-answer"
            ],
            "dependencies": [],
            "targets": [
                "desktop",
                "mobile"
            ]
        }
    },
    "Hooks": {
        "ResourceLoaderTestModules": "RatePageHooks::onResourceLoaderTestModules",
        "PageViewUpdates" : "RatePageHooks::onPageViewUpdates",
        "BeforePageDisplay": "RatePageHooks::onBeforePageDisplay",
        "InfoAction": "RatePageHooks::onInfoAction",
        "LoadExtensionSchemaUpdates": "RatePageHooks::onLoadExtensionSchemaUpdates"
    },
    "APIModules": {
        "pagerating": "ApiPageRating",
        "voteonpage": "ApiVoteOnPage"
	},
    "type": "other"
}
