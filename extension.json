{
    "name": "RatePage",
    "author": [
        "Ostrzyciel",
        "Polskacafe"
    ],
    "license-name": "MIT",
    "version": "0.2.0",
    "namemsg": "ratePage",
    "descriptionmsg": "ratePage-desc",
    "url": "https://www.mediawiki.org/wiki/Extension:RatePage",
    "manifest_version": 1,
    "MessagesDirs": {
        "RatePage": [ "i18n" ]
    },
    "config": {
        "RPRatingAllowedNamespaces": null,
        "RPRatingPageBlacklist": [],
        "RPSidebarPosition": 2,
        "RPRatingMin": 1,
        "RPRatingMax": 5,
        "RPFrontendEnabled": true,
        "RPAddSidebarSection": true
	},
    "AutoloadClasses": {
        "RatePageRating": "includes/RatePageRating.php",
        "RatePageHooks": "includes/RatePageHooks.php",
        "ApiPageRating": "includes/ApiPageRating.php"
    },
    "ResourceFileModulePaths": {
        "localBasePath": "modules",
        "remoteExtPath": "RatePage/modules"
    },
    "ResourceModules": {
        "ext.ratePage": {
            "scripts": [
                "ext.ratePage.js"
            ],
            "styles": [
                "ext.ratePage.less"
            ],
            "skinStyles": {
                "timeless": [
                    "ext.ratePage.timeless.less"
                ],
                "monobook": [
                    "ext.ratePage.monobook.less"
                ],
                "minerva": [
                    "ext.ratePage.minerva.less"
                ]
            },
            "messages": [
                "ratePage",
                "ratePage-desc",
                "apihelp-pagerating-summary",
                "apihelp-pagerating-param-pageid",
                "apihelp-pagerating-param-pagetitle",
                "apihelp-pagerating-param-answer",
                "apihelp-ratepage-example-1",
                "apihelp-ratepage-example-2",
                "ratePage-vote-error",
                "ratePage-vote-title",
                "ratePage-vote-info",
                "ratePage-vote-average-info",
                "ratePage-caption-1",
                "ratePage-caption-2",
                "ratePage-caption-3",
                "ratePage-caption-4",
                "ratePage-caption-5",
                "ratePage-prompt"
            ],
            "dependencies": [
                "mediawiki.api"
            ],
            "targets": [
                "desktop",
                "mobile"
            ]
        }
    },
    "Hooks": {
        "ResourceLoaderTestModules": "RatePageHooks::onResourceLoaderTestModules",
        "BeforePageDisplay": "RatePageHooks::onBeforePageDisplay",
        "LoadExtensionSchemaUpdates": "RatePageHooks::onLoadExtensionSchemaUpdates",
        "SkinBuildSidebar": "RatePageHooks::onSkinBuildSidebar"
    },
    "APIModules": {
        "pagerating": "ApiPageRating"
	},
    "type": "other"
}
